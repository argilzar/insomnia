{
  "name": "the-source-public-webhook-sink",
  "version": "0.1.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "proto": "protoc --proto_path=./src/assets/messages --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=emitImportedFiles=true --ts_proto_opt=nestJs=true --ts_proto_opt=addNestjsRestParameter=true --ts_proto_out=./src/messages ./src/assets/**/*.proto",
    "predocker": "yarn build",
    "docker": "docker build . -t the-source-public-webhook-sink:$(cat package.json | jq -r .version)"
  },
  "dependencies": {
    "@jbiskur/nestjs-async-module": "3.7.0",
    "@jbiskur/nestjs-options-module-factory": "3.7.0",
    "@nestjs/common": "9.0.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.1",
    "@nestjs/microservices": "9.0.8",
    "@nestjs/platform-express": "9.0.1",
    "dayjs": "1.11.3",
    "joi": "17.6.0",
    "nats": "2.7.1",
    "nest-winston": "1.6.2",
    "protobufjs": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.39",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.0.3",
    "prettier": "2.7.0",
    "source-map-support": "0.5.21",
    "supertest": "6.2.3",
    "ts-jest": "28.0.1",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
    "ts-proto": "1.117.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.3"
  }
}
